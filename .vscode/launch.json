  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Node",
      "windows": {
        "outputCapture": "std",
      },
      "program": "."
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "webRoot": "${workspaceFolder}",
      "enableContentValidation": false,
      "url": "http://localhost:5173"
    }
  ]
}